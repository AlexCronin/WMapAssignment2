<?xml version='1.0' encoding='utf-8'?>
<widget id="ie.dit.wmap20182" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Points of Interest2</name>
    <description>
        A sample Apache Cordova application for WMAP2018.
    </description>
    <author email="alex.cronin@mydit.ie" href="https://github.com/AlexCronin/WMapAssignment2/tree/master/wmap2019-cordova-master">
        Alex Cronin
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <plugin name="cordova-plugin-device" spec="~1.1.4" />
    <plugin name="cordova-plugin-dialogs" spec="~1.3.1" />
    <plugin name="cordova-plugin-geolocation" spec="~2.4.1" />
    <icon src="www/img/logo.png" />
    <engine name="android" spec="^6.4.0" />
    <engine name="browser" spec="^5.0.4" />
</widget>
